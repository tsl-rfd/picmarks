<h1>Add a New Picture</h1>

<!-- <form action="/pictures" method="post"> -->
<%= form_tag "/pictures", "method" => "post" do %>
  <!-- <label for="url_input">URL:</label><input type="text" name="url" id="url_input"> -->
  <%= label_tag "url", "URL:" %><%= text_field_tag "url" %>
  <!-- <label for="note_input">Note:</label><input type="text" name="note" id="note_input"> -->
  <%= label_tag "note", "Note:" %> <%= text_field_tag "note" %>
  <!-- <input type="submit" value="Save Picture"> -->
  <%= submit_tag "Save Picture"%>
<!-- </form> -->
<% end %>